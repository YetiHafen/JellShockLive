ButtonStyle: {
	default: {
		up: button
		down: button-red
		over: button-blue
	}
}

TextButtonStyle: {
	default: {
		font: font
		fontColor: black
		//downFontColor: text-selected
		up: button
		down: button
		over: button
	}
	red: {
		font: font
